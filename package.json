{
  "name": "v-wave",
  "version": "2.0.0",
  "description": "The material-ripple directive for Vue that actually works",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/es/index.js",
  "unpkg": "dist/iife/index.js",
  "types": "dist/types/",
  "files": [
    "dist/**/*",
    "nuxt/**/*"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "rollup --config",
    "dev": "npm run build -- --watch",
    "format": "prettier --write \"src/**/*.(t|j)s\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build  && husky install",
    "prepublishOnly": "npm test && npm run lint",
    "release": "npm run lint && npm run format && npm run test && standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justintaddei/v-wave.git"
  },
  "keywords": [
    "vue",
    "directive",
    "wave",
    "ripple",
    "v-ripple",
    "vue-ripple",
    "vue-rip",
    "v-ripple-effect",
    "v-ripple-directive",
    "material",
    "material ripple",
    "animation"
  ],
  "author": "Justin Taddei <justin@justintaddei.com> (https://justintaddei.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justintaddei/v-wave/issues"
  },
  "homepage": "https://github.com/justintaddei/v-wave#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/jest": "^29.5.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "standard-version": "^9.1.0",
    "ts-jest": "^29.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.20"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2"
  }
}
